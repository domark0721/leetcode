var twoSum = function(numbers, target) {
    for(let a = 0; a < numbers.length; a++){
        for(let b = 1; b < numbers.length; b++) {
            if ( numbers[a] + numbers[b] === target && a !== b){
                return ([a+1, b+1])
            }
        }
    }
};

console.log(twoSum([2, 7, 11, 15], 9))